<div class="container">
  <div class="index-my-rentals">
    <h1>My rentals</h1>
    <% @rentals.each do |rental| %>
      <div class="rental-card">
        <div class="picofpokerental" style="background-image: url('<%= cl_image_path rental.pokemon.race.photo%>')">
        </div>
        <div class="pokeinfosrental">
          <p><%= rental.pokemon.name %></p>
          <p><%= rental.pokemon.race.name %></p>
          <p><%= rental.user.first_name %> <%= rental.user.first_name %></p>
        </div>
      </div>
    <% end %>
  </div>
</div>